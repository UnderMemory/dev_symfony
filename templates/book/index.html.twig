{% extends 'base.html.twig' %}

{% block title %}{{parent()}} Home !{% endblock %}

{% block body %}

{% for d in data %}

    {% if loop.index0 == 0 %}
        <p style='color:hotpink'>{{ d.name }} {{ d.action }} </p>
    {% else %}
        <p>{{ d.name }} {{ d.action }} </p>
    {% endif %}

{% endfor %}

{% endblock %}